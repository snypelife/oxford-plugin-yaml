  },
  "bugs": {
    "url": "https://github.com/snypelife/oxford-plugin-yaml/issues"
  },
  "homepage": "https://github.com/snypelife/oxford-plugin-yaml",
  "devDependencies": {
    "chai": "^3.4.1",
    "coveralls": "^2.11.4",
    "eslint": "^1.10.1",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4",
    "opener": "^1.4.1",
    "oxford": "^1.6.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  }
